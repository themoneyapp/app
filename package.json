{
  "name": "themoneyapp",
  "version": "0.1.0",
  "dependencies": {
    "flowbite": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@popperjs/core": "^2.10.2",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass-tilde-importer": "^1.0.2",
    "postcss": "^8.3.11",
    "postcss-loader": "^8.0.0",
    "postcss-preset-env": "^9.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.43.4",
    "sass-loader": "^14.0.0",
    "tailwindcss": "^3.4.3",
    "webpack": "^5.65.0",
    "webpack-bundle-tracker": "^3.0.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": "20"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "prettier": {
    "printWidth": 119,
    "singleQuote": false,
    "tabWidth": 2
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "scripts": {
    "dev": "concurrently npm:dev:*",
    "dev:webpack": "webpack serve --config webpack/dev.config.js",
